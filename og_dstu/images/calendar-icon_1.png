<script type="text/javascript">
    // if ($( window ).width() < 900) {
        const rootApp = new Vue({
          el: '#vue-menu-mobile',
          data () {
            return {
                menuState: {
                    menuOpen: false,
                    lastClicked: null,
                    firstColumn: {
                        list: []
                    },
                    secondColumn: {
                        list: []
                    },
                    thirdColumn: {
                        list: []
                    },
                    fourthColumn: {
                        list: []
                    },
                }
            }
        },
        methods: {
            closeHandler: function(event) {
                $('.vmm-column:not(.column-1)').removeClass('right left active').addClass('offscreen');
                if ($(event.target).closest('.hamburger').hasClass('is-active')) {
                    $('body').removeClass('pos-fix');
                } else {
                    $('body').addClass('pos-fix');
                }
            },
            menuBack: function(event) {
                var closestColumn = $(event.target).closest('.vmm-column');
                if (closestColumn.hasClass('column-2')) {
                    if (closestColumn.hasClass('right')) {
                        closestColumn.removeClass('right').addClass('offscreen');
                    } else if (closestColumn.hasClass('left')) {
                        $('.vmm-column:not(.column-1)').removeClass('right').addClass('offscreen');
                        closestColumn.removeClass('left').addClass('right');
                    }
                } else if (closestColumn.hasClass('column-3')) {
                    if (closestColumn.hasClass('right')) {
                        $('.vmm-column:not(.column-1)').removeClass('left').addClass('right');
                        closestColumn.removeClass('right').addClass('offscreen');
                    } else if (closestColumn.hasClass('left')) {
                        $('.vmm-column:not(.column-1)').removeClass('right').addClass('offscreen');
                        closestColumn.removeClass('left').addClass('right');
                    }
                } else if (closestColumn.hasClass('column-4')) {
                    if (closestColumn.hasClass('right')) {
                        $('.vmm-column:not(.column-1)').removeClass('left').addClass('right');
                        closestColumn.removeClass('right').addClass('offscreen');
                    } else if (closestColumn.hasClass('left')) {
                        $('.vmm-column:not(.column-1)').removeClass('right').addClass('offscreen');
                        closestColumn.removeClass('left').addClass('right');
                    }
                }
            },
            menuHandler: function(item, event) {
                // console.log(item.SUBMENU);

                if ($(event.target).hasClass('arrow')) {
                    event.preventDefault();
                $(event.target).closest('.vmm-column')
                .find('.vmm-item a.active').removeClass('active');
                $(event.target).addClass('active');
                if ($(event.target).closest('.vmm-column').hasClass('column-1')) {
                    this.menuState.secondColumn.list = item.SUBMENU;
                    $('.vmm-column.column-2').removeClass('offscreen').addClass('right active');
                } else if ($(event.target).closest('.vmm-column').hasClass('column-2')) {
                    this.menuState.thirdColumn.list = item.SUBMENU;
                    $('.vmm-column.column-2').removeClass('right').addClass('left');
                    $('.vmm-column.column-3').removeClass('offscreen').addClass('right active');
                } else if ($(event.target).closest('.vmm-column').hasClass('column-3')) {
                    this.menuState.fourthColumn.list = item.SUBMENU;
                    $('.vmm-column.column-3').removeClass('right').addClass('left');
                    $('.vmm-column.column-4').removeClass('offscreen').addClass('right active');
                }
                // this.menuState.lastClicked = event.currentTarget;

            }
        },
        getMenu: function() {
            var self = this;
            $.getJSON( "/include/mobile.menu/index.php?lang=63", function( data ) {
                console.log(data);
                self.menuState.firstColumn.list = data;
            });
/*            var self = this,
            hostName = window.location.hostname;
            axios.get('/include/mobile.menu/index.php').then(function(response) {
                self.list = response.data;
            }, function(error) {
                console.log(error.statusText);
            });*/
        }
    },
    mounted: function() {
        this.getMenu();
    },
    updated: function() {
        $(".vmm-column").mCustomScrollbar({
            mouseWheelPixels: 105,
            contentTouchScroll: 1,
            documentTouchScroll: true,
            callbacks:{
              onScrollStart: function(){
                console.log('scroll start');
              }
          }
        });
        $('.vmm-column').each(function(index, el) {
            $(this).find('.vmm-back').detach().prependTo($(this));
        });
    }
})
    // }
